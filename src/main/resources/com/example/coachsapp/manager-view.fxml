<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.coachsapp.controller.ManagerController"
      spacing="10" style="-fx-padding: 20;">

    <Label text="Manager List" style="-fx-font-size: 24; -fx-font-weight: bold;"/>

    <ListView fx:id="managerList" VBox.vgrow="ALWAYS"/>

    <HBox spacing="10">
        <Button text="Add Manager" onAction="#addManager"/>
        <Button text="Delete Manager" onAction="#deleteManager"/>
        <Region HBox.hgrow="ALWAYS"/>
        <Button text="Back" onAction="#goBack"/>
    </HBox>
</VBox>

